) Letter","agencyId":"clos7y8et0005460xv4bmqwqj","parentTopicId":null,"priority":0,"subtopics":[{"agencyId":"clos7y8et0005460xv4bmqwqj","id":507,"parentTopicId":506,"priority":0,"qcount":0,"title":"After submitting HLE application"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":508,"parentTopicId":506,"priority":0,"qcount":0,"title":"Submission of an HLE application"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":509,"parentTopicId":506,"priority":0,"qcount":0,"title":"Submission of documents"}]},{"id":492,"qcount":0,"title":"HDB Resale Portal","agencyId":"clos7y8et0005460xv4bmqwqj","parentTopicId":null,"priority":0,"subtopics":[{"agencyId":"clos7y8et0005460xv4bmqwqj","id":500,"parentTopicId":492,"priority":0,"qcount":0,"title":"Accepting Resale Application"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":505,"parentTopicId":492,"priority":0,"qcount":0,"title":"Accessing the HDB InfoWEB / HDB Resale Portal"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":498,"parentTopicId":492,"priority":0,"qcount":0,"title":"Acknowledging Resale Documents by Buyers"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":497,"parentTopicId":492,"priority":0,"qcount":0,"title":"Acknowledging Resale Documents by Sellers"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":503,"parentTopicId":492,"priority":0,"qcount":0,"title":"Choosing Mode of Financing"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":496,"parentTopicId":492,"priority":0,"qcount":0,"title":"Endorsing Resale Documents"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":499,"parentTopicId":492,"priority":0,"qcount":0,"title":"Inspecting Flat"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":495,"parentTopicId":492,"priority":0,"qcount":0,"title":"Making Payment"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":504,"parentTopicId":492,"priority":0,"qcount":0,"title":"Registering Intent to Sell or Intent to Buy"},{"agencyId":"clos7y8et0005460xv4bmqwqj","id":502,"parentTopicId":492,"priority":0,"qcount":0,"title":"Requesting for Value of Flat"},{"agencyI